{
    "name": "distilleries/expendable",
    "description": "Expendable is an admin panel base on laravel 5.*. This package give you some implementation do add a content management system of your application. You can override everything. This Cms give view few tools to develop your content management easily and properly.",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel",
        "admin",
        "form-generator",
        "datatable-generator",
        "expendable",
        "distilleries",
        "gulp"
    ],
    "authors": [
        {
            "name": "Maxime Francois",
            "email": "kezho@me.com"
        }
    ],

    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "illuminate/translation": "5.*",
        "distilleries/datatable-builder": "2.*",
        "distilleries/form-builder": "2.*",
        "distilleries/mailersaver": "2.*",
        "distilleries/permission-util": "1.*",
        "forxer/Gravatar": "~1.2",
        "maatwebsite/excel": "2.0.*"
    },
    "require-dev": {
        "orchestra/database": "~3.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "1.4.*"
    },
    "autoload": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "Distilleries\\Expendable\\": "src/Distilleries/Expendable"
        }
    },
    "minimum-stability": "stable"
}
